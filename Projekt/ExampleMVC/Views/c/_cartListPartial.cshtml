@model IEnumerable<Example.Entities.Models.Cart>

<table class="table">
    <tr>
        <th>ProductId</th>
        <th>Quantity</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.ProductId
            </td>
            <td>
                @item.Quantity
            </td>
        </tr>
    }

</table>